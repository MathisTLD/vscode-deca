{
  "name": "deca",
  "displayName": "Deca",
  "description": "Language Support for Deca",
  "author": "MathisTLD",
  "icon": "icons/kikslamenace.png",
  "publisher": "Mathis",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "deca",
        "aliases": [
          "Deca",
          "deca"
        ],
        "extensions": [
          ".deca",
          ".decah"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/deca-light.png",
          "dark": "./icons/deca-dark.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "deca",
        "scopeName": "source.deca",
        "path": "./syntaxes/deca.tmLanguage.json"
      }
    ]
  },
  "configurationDefaults": {
    "[deca]": {
      "editor.semanticHighlighting.enabled": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MathisTLD/vscode-deca.git"
  }
}
